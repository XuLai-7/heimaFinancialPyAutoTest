{
  "img_code": [{
    "desc": "获取图片验证码成功（随机小数）",
    "random": 0.123,
    "expect_result": 200
  },{
    "desc": "获取图片验证码成功（随机整数）",
    "random": 123,
    "expect_result": 200
  },{
    "desc": "获取图片验证码失败（随机数为空）",
    "random": "",
    "expect_result": 404
  },{
    "desc": "获取图片验证码失败（随机数为字符串）",
    "random": "hellotest",
    "expect_result": 400
  }],
  "phone_code": [{
    "desc": "获取短信验证码成功",
    "phone":13600001111,
    "imgVerifyCode": 8888,
    "expect_result": "发送成功"
  },{
    "desc": "获取短信验证码失败（图片验证码错误）",
    "phone":13600001111,
    "imgVerifyCode": 8889,
    "expect_result": "验证码错误"
  }],
  "register": [{
    "desc": "注册成功（必填参数)",
    "phone":13600001111,
    "password": "test123",
    "verifycode": 8888,
    "phone_code": 666666,
    "expect_result": "注册成功"
  },{
    "desc": "注册失败（图片验证码错误）",
    "phone":13600001112,
    "password": "test123",
    "verifycode": 8889,
    "phone_code": 666666,
    "expect_result": "验证码错误"
  },{
    "desc": "注册失败（短信验证码错误）",
    "phone":13600001112,
    "password": "test123",
    "verifycode": 8889,
    "phone_code": 666667,
    "expect_result": "验证码错误"
  },{
    "desc": "注册失败（手机号已存在）",
    "phone":13600001111,
    "password": "test123",
    "verifycode": 8888,
    "phone_code": 666666,
    "expect_result": "已存在"
  }],
  "login": [
    {
      "desc": "登录成功",
      "keywords":13600001111,
      "password": "test123",
      "expect_result": "登录成功"
    },
    {
      "desc": "登录失败（密码为空）",
      "keywords":13600001111,
      "password": "",
      "expect_result": "不能为空"
    },
    {
      "desc": "登录失败（解锁）",
      "keywords":13600001111,
      "password": "error123",
      "expect_result": "登录成功"
    }],
  "login_status": [
    {
      "desc": "查询登录状态（已登录）",
      "status": "已登录",
      "expect_result": "OK"
    },
    {
       "desc": "查询登录状态（未登录）",
       "status": "未登录",
       "expect_result": "未登"
    }
  ]
}